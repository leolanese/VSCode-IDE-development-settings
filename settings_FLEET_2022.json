{
    "launchTerminalOnOpen": true,
    "terminal.enableFocusEditorOnEsc": true,
    "fus.sendAllowed": false,
    "theme": "dark_purple",
    "editor.alwaysShowHorizontalScrollbar": true,
    "editor.showFloatingDebugPanel": false,
    "editor.showTooltipOnHover": true,
    "editor.showScrollbarMarks" true,
    "editor.showBreakpoints": true,
    "editor.showNavBar": true,
    "editor.showLineNumbers": true,
    "editor.showStatusBar": true,
    "editor.fontSize": 14,
    "editor.showFileStatus": true,
    "editor.softWrapEnabled": true,
    "editor.showDeclarationUsages": true,
    "editor.selectIdentifierOnWordSelection": true,
    "completion.autoPopupDelay": 2,
    "excluded": ["/node_modules", "/coverage", "/tmp", "/.idea", "/.history/", "/tmp", "/.sass-cache", "/out-tsc", ".cache/"],
    "git.autoFetch": true,
    "[HTML]": {
        "editor.softWrapEnabled": true
    }
}
